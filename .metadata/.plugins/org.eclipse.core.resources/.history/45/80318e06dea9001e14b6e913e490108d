package ex03;

/*
 * int -> char unicode A =>65 65=>A
 * 실수->정수
 * double -> int 소수점을 내려서 형변환됩니다.
 */
public class Ex08 {

	public static void main(String[] args) {
		double d = 85.4d;
		int score = (int)d;
		System.out.println("score = "+score);
		System.out.println("d= "+d);
		
		int aa = 10;
		double ee = aa;
		System.out.println("aa="+aa);
		System.out.println("ee="+ee);
	}
}
